          <parameter name="tableName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[name of the ovsdb table]]>
          </parameter>
          <parameter name="uuid" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[UUID of the Row to be removed]]>
          </parameter>
          <outValue>
            
            
            <documentation><![CDATA[Response as dictated by the HTTP Response Status code

<pre>
Example:

Request URL:
https://localhost/controller/nb/v2/ovsdb/tables/bridge/rows/41ab15a9-0dab-4675-a579-63019d4bcbec

</pre>]]></documentation>
          </outValue>
          <statusCode code="204">
            <![CDATA[User Deleted Successfully]]>
          </statusCode>
          <statusCode code="401">
            <![CDATA[User not authorized to perform this operation]]>
          </statusCode>
          <statusCode code="404">
            <![CDATA[The userName passed was not found]]>
          </statusCode>
          <statusCode code="500">
            <![CDATA[Internal Server Error : Removal of user failed]]>
          </statusCode>
          <statusCode code="503">
            <![CDATA[Service unavailable]]>
          </statusCode>
        </operation>
      </resource>
    </resources>
    <groups>
      <group>
        <name>OVSDBNorthbound</name>
        <documentation><![CDATA[This class provides REST APIs to Create, Read, Update and Delete OVSDB Row in any of the ovsdb table
database one at a time.]]></documentation>
      </group>
    </groups>
