  <rest baseAddress="http://localhost:8080/full" wadl="application.wadl">
    <resources>
      <resource name="/node/{nodeType}/{nodeId}/tables/{tableName}/rows" subcontext="/oc/nb/v2">
        <groups>
          <group>OVSDBNorthbound</group>
        </groups>
        <operation name="POST">
          <documentation>
            <![CDATA[Create a Row]]>
          </documentation>
          <contentType type="*/*" consumable="false" produceable="true"/>
          <contentType type="application/xml" consumable="false" produceable="true"/>
          <contentType type="application/json" consumable="true" produceable="false"/>
          <parameter name="nodeType" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[(no documentation provided)]]>
          </parameter>
          <parameter name="nodeId" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[(no documentation provided)]]>
          </parameter>
          <parameter name="tableName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[name of the ovsdb table]]>
          </parameter>
          <inValue>
            
            
            <documentation><![CDATA[the OVSDBRow Row that is being inserted]]></documentation>
          </inValue>
          <outValue>
            
            
            <documentation><![CDATA[Response as dictated by the HTTP Response Status code

<pre>
Example:

Request URL:
https://localhost/controller/nb/v2/ovsdb/tables/bridge/rows
</pre>]]></documentation>
          </outValue>
          <statusCode code="201">
            <![CDATA[Row Inserted successfully]]>
          </statusCode>
          <statusCode code="400">
            <![CDATA[Invalid data passed]]>
          </statusCode>
          <statusCode code="401">
            <![CDATA[User not authorized to perform this operation]]>
          </statusCode>
        </operation>
        <operation name="GET">
          <documentation>
            <![CDATA[Read all Rows of a table]]>
          </documentation>
          <contentType type="*/*" consumable="false" produceable="true"/>
          <contentType type="application/xml" consumable="false" produceable="true"/>
          <contentType type="application/json" consumable="true" produceable="false"/>
          <parameter name="nodeType" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[(no documentation provided)]]>
          </parameter>
          <parameter name="nodeId" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[(no documentation provided)]]>
          </parameter>
          <parameter name="tableName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[name of the ovsdb table]]>
          </parameter>
          <outValue>
            
            
            <documentation><![CDATA[All the Rows of a table

<pre>
Example:

Request URL:
https://localhost/controller/nb/v2/ovsdb/tables/bridge/rows
</pre>]]></documentation>
